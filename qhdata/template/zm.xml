<?xml version="1.0" encoding="UTF-8"?>
<瑲珩 版本號="0.2">
	<範本集>
		<!-- 基本範本：線形 -->
		<範本 名稱="範衍">
			<參數列>
				<參數 名稱="行"/>
				<參數 名稱="氵"/>
			</參數列>
			<組字結構>
				<組字 運算="龍">
					<字根 置換="行"/>
					<字根 置換="氵"/>
				</組字>
			</組字結構>
		</範本>

		<範本 名稱="範胤">
			<參數列>
				<參數 名稱="儿"/>
				<參數 名稱="幺"/>
			</參數列>
			<組字結構>
				<組字 運算="龍">
					<字根 置換="儿"/>
					<字根 置換="幺"/>
				</組字>
			</組字結構>
		</範本>

		<範本 名稱="範渊">
			<參數列>
				<參數 名稱="乂"/>
				<參數 名稱="米"/>
			</參數列>
			<組字結構>
				<組字 運算="龍">
					<字根 置換="[丿丨]"/>
					<字根 置換="米"/>
				</組字>
			</組字結構>
		</範本>

		<!-- 範本：引數在右下方 -->
		<範本 名稱="範翛">
			<參數列>
				<參數 名稱="羽"/>
			</參數列>
			<組字結構>
				<組字 運算="範湘">
					<字根 置換="亻"/>
					<字根 置換="丨"/>
					<組字 運算="範志">
						<字根 置換="夂"/>
						<字根 置換="羽"/>
					</組字>
				</組字>
			</組字結構>
		</範本>
		<!-- 範本： -->

		<!-- 範本： -->
		<範本 名稱="範斎">
			<參數列>
				<參數 名稱="示"/>
			</參數列>
			<組字結構>
				<組字 運算="龍">
					<字根 置換="齐"/>
					<字根 置換="示"/>
				</組字>
			</組字結構>
		</範本>
	</範本集>
</瑲珩>
