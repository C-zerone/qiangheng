<?xml version="1.0" encoding="UTF-8"?>
<瑲珩 版本號="0.2">
	<範本集>
		<!-- 基本運算： -->
		<範本 名稱="範起">
			<參數列>
				<參數 名稱="走"/>
				<參數 名稱="巳"/>
			</參數列>
			<組字結構>
				<條件式 運算="=" 運算元一="走" 運算元二="辶"/>
				<組字 運算="龍">
					<字根 置換="巳"/>
					<字根 置換="走"/>
				</組字>
			</組字結構>
			<組字結構>
				<條件式 運算="=" 運算元一="走" 運算元二="廴"/>
				<組字 運算="龍">
					<字根 置換="巳"/>
					<字根 置換="走"/>
				</組字>
			</組字結構>
			<組字結構>
				<條件式 運算="=" 運算元一="走" 運算元二="[亾下]"/>
				<組字 運算="龍">
					<字根 置換="巳"/>
					<字根 置換="走"/>
				</組字>
			</組字結構>
			<組字結構>
				<組字 運算="龍">
					<字根 置換="走"/>
					<字根 置換="巳"/>
				</組字>
			</組字結構>
		</範本>

		<範本 名稱="範胤">
			<參數列>
				<參數 名稱="儿"/>
				<參數 名稱="幺"/>
			</參數列>
			<組字結構>
				<組字 運算="龍">
					<字根 置換="儿"/>
					<字根 置換="幺"/>
				</組字>
			</組字結構>
		</範本>

		<!-- 範本：引數在下邊中間 -->
		<範本 名稱="範威">
			<參數列>
				<參數 名稱="女"/>
			</參數列>
			<組字結構>
				<組字 運算="同">
					<字根 置換="戌"/>
					<字根 置換="女"/>
				</組字>
			</組字結構>
		</範本>

		<!-- 範本： -->
		<範本 名稱="範䜌">
			<參數列>
				<參數 名稱="言"/>
				<參數 名稱="糸"/>
			</參數列>
			<組字結構>
				<組字 運算="範湘">
					<字根 置換="言"/>
					<字根 置換="糸"/>
					<字根 置換="糸"/>
				</組字>
			</組字結構>
		</範本>

		<!-- 範本： -->
		<範本 名稱="範贏">
			<參數列>
				<參數 名稱="貝"/>
			</參數列>
			<組字結構>
				<組字 運算="範志">
					<字根 置換="吂"/>
					<組字 運算="範湘">
						<字根 置換="月"/>
						<字根 置換="貝"/>
						<字根 置換="凡"/>
					</組字>
				</組字>
			</組字結構>
		</範本>
	</範本集>
</瑲珩>
