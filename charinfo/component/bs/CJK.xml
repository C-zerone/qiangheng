<?xml version="1.0" encoding="UTF-8"?>
<瑲珩 版本號="0.1">
	<範本集>
		<!-- 基本運算： -->
		<範本 名稱="範起">
			<參數列>
				<參數 名稱="走"/>
				<參數 名稱="巳"/>
			</參數列>
			<組字結構>
				<條件式 運算="=" 運算元一="走" 運算元二="辶"/>
				<組字 運算="龍">
					<字根 置換="巳"/>
					<字根 置換="走"/>
				</組字>
			</組字結構>
			<組字結構>
				<條件式 運算="=" 運算元一="走" 運算元二="廴"/>
				<組字 運算="龍">
					<字根 置換="巳"/>
					<字根 置換="走"/>
				</組字>
			</組字結構>
			<組字結構>
				<組字 運算="龍">
					<字根 置換="走"/>
					<字根 置換="巳"/>
				</組字>
			</組字結構>
		</範本>

		<!-- 範本： -->
		<範本 名稱="範斎">
			<參數列>
				<參數 名稱="示"/>
			</參數列>
			<組字結構>
				<組字 運算="範志">
					<字根 置換="文"/>
					<組字 運算="龍">
						<字根 置換="二"/>
						<字根 置換="示"/>
					</組字>
				</組字>
			</組字結構>
		</範本>
		<範本 名稱="範微">
			<參數列>
				<參數 名稱="几"/>
			</參數列>
			<組字結構>
				<組字 運算="龍">
					<字根 置換="[特微]"/>
					<字根 置換="丨"/>
					<字根 置換="几"/>
					<字根 置換="攵"/>
				</組字>
			</組字結構>
		</範本>
		<範本 名稱="範贏">
			<參數列>
				<參數 名稱="貝"/>
			</參數列>
			<組字結構>
				<組字 運算="範志">
					<字根 置換="吂"/>
					<組字 運算="範湘">
						<字根 置換="月"/>
						<字根 置換="貝"/>
						<字根 置換="卂"/>
					</組字>
				</組字>
			</組字結構>
		</範本>

		<!-- 範本： -->
		<範本 名稱="範戀">
			<參數列>
				<參數 名稱="言"/>
				<參數 名稱="糸"/>
			</參數列>
			<組字結構>
				<組字 運算="範湘">
					<字根 置換="糸"/>
					<字根 置換="言"/>
					<字根 置換="糸"/>
				</組字>
			</組字結構>
		</範本>
	</範本集>
	<字符集>
		<!--4F00-->
		<字符 名稱="來" 註記="U+4F86">
			<組字/>
		</字符>
		<!--5400-->
		<字符 名稱="吂" 註記="U+5402">
			<組字/>
		</字符>
		<!--9A00-->
		<字符 名稱="髟" 註記="U+9ADF">
			<組字/>
		</字符>
		<!--自訂字根-->
		<字符 名稱="[紁右]" 註記="[紁右]">
			<組字 運算="爲">
				<字根 置換="义"/>
			</組字>
		</字符>
		<字符 名稱="[刼右]" 註記="[刼右]">
			<組字 運算="爲">
				<字根 置換="刄"/>
			</組字>
		</字符>
		<字符 名稱="[将右]" 註記="[将右]">
			<組字 運算="爲">
				<字根 置換="寽"/>
			</組字>
		</字符>
	</字符集>
</瑲珩>
