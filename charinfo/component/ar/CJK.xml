				<組字 運算="龍">
					<字根 置換="厂"/>
					<字根 置換="[一口]"/>
					<字根 置換="[特戈]"/>
				</組字>
			</組字結構>
			<組字結構>
				<組字 運算="龍">
					<字根 置換="厂"/>
					<字根 置換="一"/>
					<字根 置換="女"/>
					<字根 置換="[特戈]"/>
				</組字>
			</組字結構>
