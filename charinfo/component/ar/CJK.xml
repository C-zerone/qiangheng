<?xml version="1.0" encoding="UTF-8"?>
<瑲珩 版本號="0.1">
	<範本集>
		<!-- 基本運算： -->
		<範本 名稱="範起">
			<參數列>
				<參數 名稱="走"/>
				<參數 名稱="巳"/>
			</參數列>
			<組字結構>
				<條件式 運算="=" 運算元一="走" 運算元二="辶"/>
				<組字 運算="龍">
					<字根 置換="巳"/>
					<字根 置換="走"/>
				</組字>
			</組字結構>
			<組字結構>
				<條件式 運算="=" 運算元一="走" 運算元二="廴"/>
				<組字 運算="龍">
					<字根 置換="巳"/>
					<字根 置換="走"/>
				</組字>
			</組字結構>
			<組字結構>
				<組字 運算="龍">
					<字根 置換="走"/>
					<字根 置換="巳"/>
				</組字>
			</組字結構>
		</範本>

		<!-- 範本：引數在左下方 -->
		<範本 名稱="範殻">
			<參數列>
				<參數 名稱="几"/>
			</參數列>
			<組字結構>
				<組字 運算="範好">
					<組字 運算="範志">
						<字根 置換="[士冖]"/>
						<字根 置換="几"/>
					</組字>
					<字根 置換="殳"/>
				</組字>
			</組字結構>
		</範本>
		<範本 名稱="範穀">
			<參數列>
				<參數 名稱="禾"/>
			</參數列>
			<組字結構>
				<條件式 運算="=" 運算元一="禾" 運算元二="口"/>
				<組字 運算="範好">
					<組字 運算="範志">
						<字根 置換="[士冖]"/>
						<字根 置換="[一口]"/>
					</組字>
					<字根 置換="殳"/>
				</組字>
			</組字結構>
			<組字結構>
				<組字 運算="範好">
					<組字 運算="範算">
						<字根 置換="[士冖]"/>
						<字根 置換="一"/>
						<字根 置換="禾"/>
					</組字>
					<字根 置換="殳"/>
				</組字>
			</組字結構>
		</範本>
		<範本 名稱="範忒">
